	},
	"license": "MIT",
	"name": "moodle_userscript",
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": false,
		"jsxSingleQuote": true,
		"singleQuote": true,
		"trailingComma": "all",
		"useTabs": true
	},
	"private": true,
	"scripts": {
		"build": "yarn fmt && yarn run webpack",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"pretest": "rimraf dist/test && tsc --project test/tsconfig.json",
		"fmt": "prettier -w . && xo --fix",
		"lint": "xo",
		"serve": "serve dist/userscript-out",
		"start": "concurrently yarn:webpack:w yarn:serve -r -k",
		"webpack": "webpack -c production.config.js --env PROD",
		"webpack:w": "webpack -c production.config.js -w"
	},
	"type": "module",
	"version": "1.0.0",
	"jest": {
		"transform": {},
		"testMatch": [
			"<rootDir>/dist/test/test/**/*.test.js"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/test/moodle-functions-v3/"
		]
	}
}
