{
	"author": "melusc",
	"dependencies": {
		"clsx": "^1.2.1",
		"dayjs": "^1.11.7",
		"dompurify": "^2.4.3",
		"file-saver": "^2.0.5",
		"htm": "^3.1.1",
		"immer": "^9.0.19",
		"jszip": "^3.10.1",
		"preact": "^10.11.3"
	},
	"description": "",
	"devDependencies": {
		"@jest/globals": "^29.4.1",
		"@lusc/tsconfig": "^2.1.0",
		"@types/dompurify": "^2.4.0",
		"@types/file-saver": "^2.0.5",
		"@types/tampermonkey": "^4.0.10",
		"clean-webpack-plugin": "^4.0.0",
		"concurrently": "^7.6.0",
		"csso-loader": "^0.3.1",
		"dotenv": "^16.0.3",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-jest": "^27.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"jest": "^29.4.1",
		"mock-violentmonkey": "^1.4.0",
		"ow": "^1.1.1",
		"prettier": "^2.8.3",
		"rimraf": "^4.1.2",
		"sass": "^1.58.0",
		"sass-loader": "^13.2.0",
		"serve": "^14.2.0",
		"terser-webpack-plugin": "^5.3.6",
		"ts-loader": "^9.4.2",
		"type-fest": "^3.5.4",
		"typescript": "^4.9.5",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"webpack-glob-entry": "^2.1.1",
		"xo": "^0.53.1"
	},
	"license": "MIT",
	"name": "moodle_userscript",
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": false,
		"jsxSingleQuote": true,
		"singleQuote": true,
		"trailingComma": "all",
		"useTabs": true
	},
	"private": true,
	"scripts": {
		"build": "yarn fmt && yarn run webpack",
		"test": "yarn build:test && node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"build:test": "rimraf dist/test && tsc --project test/tsconfig.json",
		"fmt": "prettier -w . && xo --fix",
		"lint": "xo",
		"serve": "serve dist/userscript-out -L",
		"start": "concurrently yarn:webpack:w yarn:serve -r -k",
		"webpack": "webpack -c production.config.js --env PROD",
		"webpack:w": "webpack -c production.config.js -w"
	},
	"type": "module",
	"version": "1.0.0",
	"jest": {
		"transform": {},
		"testMatch": [
			"<rootDir>/dist/test/test/**/*.test.js"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/test/moodle-functions-v3/"
		],
		"setupFiles": [
			"<rootDir>/test/set-env.js"
		]
	},
	"packageManager": "yarn@3.4.1"
}
